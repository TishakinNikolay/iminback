   "scripts": {
      "prebuild": "rimraf build",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start ",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node build/src/main.js",
      "tslint": "tslint -p tsconfig.json",
      "tslint-fix": "tslint --fix -p tsconfig.json",
      "prestart:prod": "yarn build",
      "postinstall": "yarn prestart:prod",
      "typeorm": "ts-node  ./node_modules/typeorm/cli.js",
      "rimraf": "./node_modules/rimraf/bin.js",
      "typeorm:migration:generate": "yarn typeorm -- migration:generate -n",
      "typeorm:migration:run": "yarn typeorm -- migration:run",
      "typeorm:migration:revert": "yarn typeorm -- migration:revert"
   },
